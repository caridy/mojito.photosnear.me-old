[{
    "settings": [ "master" ],

	"root": {
		"path": "/",
		"method": ["get"],
		"call": "frame.index"
	},

	"places": {
		"path": "/places/:id/",
		"method": ["get"],
		"call": "frame.index",
		"params": "type=place"
	},

	"photos": {
		"path": "/photos/:id/",
		"method": ["get"],
		"call": "frame.index",
		"params": "type=photo"
	}

}]
